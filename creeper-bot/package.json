    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "discord.js": "^12.4.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "ffmpeg-static": "^4.2.7",
    "form-data": "^3.0.0",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.0.3",
    "rxjs": "^6.6.3",
    "socket.io": "^2.3.0",
    "socketio-jwt": "^4.6.2",
    "ytdl-core": "^4.0.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.9",
    "@types/node": "^14.14.7",
    "@types/pg": "^7.14.6",
    "@types/socket.io": "^2.1.11",
    "@types/socketio-jwt": "^0.2.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "eslint": "^7.13.0",
    "tsc-watch": "^4.2.9",
    "typescript": "^3.9.7"
  }
